pipeline{
    agent any

        //label 'jenkins-node-aws'
    tools {
        maven 'Maven3' // Replace with the name you configured
    }
        
    stages{
        stage('build'){
            steps{
                script{
                    //sh 'mvn clean package'
                    sh 'echo "in progress building"'
                }
            }
        }
     stage('test'){
            steps{
                script{
                    //sh 'mvn test'
                    sh 'echo "test done"'
                }
            }
     }

    }
}
